        {"name": "keyword.datetime.tm1process", "match": "(?i)\\b(FormatDate|NewDateFormatter|ParseDate)\\b(?=\\()"},
        {"name": "keyword.dimension.tm1process", "match": "(?i)\\b(DimensionCreate|DimensionDeleteAllElements|DimensionDestroy|DimensionElementComponentAdd|DimensionElementComponentAddDirect|DimensionElementComponentDelete|DimensionElementComponentDeleteDirect|DimensionElementDelete|DimensionElementDeleteDirect|DimensionElementInsert|DimensionElementInsertDirect|DimensionElementPrincipalName|DimensionExists|DimensionTimeLastUpdated|DimensionSortOrder|DimensionUpdateDirect|DimensionTopElementInsert|DimensionTopElementInsertDirect)\\b(?=\\()"},
